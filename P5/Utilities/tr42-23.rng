    <define name="single-code-point">
        <data type="string">
            <param name="pattern">(|[1-9A-F]|(10))[0-9A-F]{4}</param>
        </data>
    </define>
    <define name="one-or-more-code-points">
        <list>
            <oneOrMore>
                <ref name="single-code-point"/>
            </oneOrMore>
        </list>
    </define>
    <define name="zero-or-more-code-points">
        <list>
            <zeroOrMore>
                <ref name="single-code-point"/>
            </zeroOrMore>
        </list>
    </define>
    <define name="two-code-points">
        <list>
            <ref name="single-code-point"/>
            <ref name="single-code-point"/>
        </list>
    </define>
    <define name="character-name">
        <data type="string">
            <param name="pattern">([A-Z0-9 #\-\(\)]*)|(&lt;control&gt;)</param>
        </data>
    </define>
    <define name="jis-code-point">
        <data type="string">
            <param name="pattern">[0-9A-F]{4}</param>
        </data>
    </define>
    <start>
        <element name="ucd">
            <ref name="ucd.content"/>
        </element>
    </start>
    <define name="boolean">
        <choice>
            <value>Y</value>
            <value>N</value>
        </choice>
    </define>
    <define combine="interleave" name="ucd.content">
        <optional>
            <element name="description">
                <text/>
            </element>
        </optional>
    </define>
    <define combine="interleave" name="ucd.content">
        <optional>
            <element name="repertoire">
                <oneOrMore>
                    <choice>
                        <ref name="code-point"/>
                        <ref name="group"/>
                    </choice>
                </oneOrMore>
            </element>
        </optional>
    </define>
    <define name="set-of-code-points">
        <choice>
            <attribute name="cp">
                <ref name="single-code-point"/>
            </attribute>
            <group>
                <attribute name="first-cp">
                    <ref name="single-code-point"/>
                </attribute>
                <attribute name="last-cp">
                    <ref name="single-code-point"/>
                </attribute>
            </group>
        </choice>
    </define>
    <define combine="choice" name="code-point">
